<%- include("_header"); -%>
<br>
<br>
<h1 align="center">เพิ่ม</h1>
<div class="container">
    <div class="row mt-5">
        <div class="col-md-12 d-flex justify-content-center">
            <form action="/deploy37/add" method="POST">
               



              


                <div class="form-group">
                    <label for="device37">Name</label>
                    <select class="form-control" name="did">
                        <option>กรุณาเลือก</option>
                        <% if(device37 && device37.length > 0) { %>
                            <% for(var i=0; i<device37.length; i++) { %>
                                <option value="<%= device37[i].did %>"><%= device37[i].Name %></option>
                            <% } %>
                        <% } %>
                    </select>
                </div>
                
                <div class="form-group">
                    <label for="site37">Namesid</label>
                    <select class="form-control" name="sid">
                        <option>กรุณาเลือก</option>
                        <% if(site37 && site37.length > 0) { %>
                            <% for(var i=0; i<site37.length; i++) { %>
                                <option value="<%= site37[i].sid %>"><%= site37[i].Name %></option>
                            <% } %>
                        <% } %>
                    </select>
                </div>
                
                
                
                
        
                
                <div class="form-group">
                    <label for="install">install:</label>
                    <input type="datetime-local" name="install" placeholder="install" class="form-control">
                </div>
                <div class="form-group">
                    <label for="comment">comment:</label>
                    <input type="text" name="comment" placeholder="comment" class="form-control">
                </div>
                <br>
                <button type="submit" class="btn btn-primary">ยืนยัน</button>
                <a href="/deploy37/list" class="btn btn-success">ย้อนกลับ</a>
            </form>
        </div>
    </div>
</div>

<%- include("_footer"); -%>